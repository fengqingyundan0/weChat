.contact{
  width: 100%;
  height: 180rpx;
  padding: 40rpx 0px;
  display: flex;
  flex-direction: row;
}
.contact_a{
  width: 50%;
  height: 180rpx;
}
.contact_a image{
  width: 32%;
  height: 120rpx;
  margin-left: 34%;
}
.contact_t{
  display: block;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  text-align: center;
}
button{
  background: none;
  padding: 0;
  line-height: 1.5;
}
button::after{ border: none; }