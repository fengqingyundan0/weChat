<!--pages/my/my.wxml-->
<view class='my'>
  <view>
    <!-- <image class='mybgdImg' src='../../images/background.png'></image> -->
    <view class="userInfo">
      <view class='avatarShell'>
        <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
      </view>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
      <!-- <view class='myInfo'>
        <view class='myInfoData'>步步币：{{currency}}</view>
        <view class='myInfoData'>连续签到：{{cci}}</view>
        <view class='myInfoData'>七日邀请：{{invitation}}</view>
      </view> -->
    </view>
  </view>
  <view style='width:100%;height:10rpx;background-color:#f5f5f5;'></view>
  <view class='my2' >
    <view class='my2s' bindtap="myExchange">
      <image class='my2Icon' src='../../images/my21.png'></image>
      <text>我的兑换</text>
    </view>
    <view class='my2s' bindtap='strategy'>
      <image class='my2Icon' src='../../images/zhanghu.png'></image>
      <text>我的账户</text>
    </view>
    <view class='my2s' bindtap=''>
      <image class='my2Icon' src='../../images/my23.png'></image>
      <text>运动打卡</text>
    </view>
    <view class='clear'></view>
  </view>

  <view>
    <view class='my3s' bindtap='gold'>
      <image class='my3Icon' src='../../images/yaoqing.png'></image>
      <text>我邀请的好友</text>
      <image class='my3Icon2' src='../../images/icon1.png'></image>
    </view>
    <view class='my3s' bindtap='ready'>
      <image class='my3Icon' src='../../images/my34.png'></image>
      <text>规则说明</text>
      <image class='my3Icon2' src='../../images/icon1.png'></image>
    </view>
    <view class='my3s' bindtap='take'>
      <image class='my3Icon' src='../../images/my33.png'></image>
      <text>收货地址</text>
      <image class='my3Icon2' src='../../images/icon1.png'></image>
    </view>
    <view class='my3s' bindtap='contact'>
      <image class='my3Icon' src='../../images/my35.png'></image>
      <text>联系我们</text>
      <image class='my3Icon2' src='../../images/icon1.png'></image>
    </view>
    <view class='my3s' bindtap='problem'>
      <image class='my3Icon' src='../../images/xiaochengxu.png'></image>
      <text>小程序推荐</text>
      <image class='my3Icon2' src='../../images/icon1.png'></image>
    </view>
  </view>
</view>